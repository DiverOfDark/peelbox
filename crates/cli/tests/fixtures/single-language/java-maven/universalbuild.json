[
  {
    "build": {
      "cache": [
        "/root/.m2/repository",
        "target"
      ],
      "commands": [
        "mvn package -DskipTests",
        "mvn dependency:copy-dependencies -DoutputDirectory=target/lib"
      ],
      "env": {
        "JAVA_HOME": "/usr/lib/jvm/java-21-openjdk",
        "MAVEN_OPTS": "-Dmaven.repo.local=/root/.m2/repository"
      },
      "packages": [
        "openjdk-21",
        "maven-3.9",
        "ca-certificates"
      ]
    },
    "metadata": {
      "build_system": "Maven",
      "framework": "Spring Boot",
      "language": "Java",
      "project_name": "example-app",
      "reasoning": "Detected from pom.xml in "
    },
    "runtime": {
      "command": [
        "java",
        "-jar",
        "/app/example-app-1.0.0.jar"
      ],
      "copy": [
        {
          "from": "target/*.jar",
          "to": "/app"
        },
        {
          "from": "target/lib/",
          "to": "/app/lib"
        }
      ],
      "env": {
        "CLASSPATH": "/app/*:/app/lib/*",
        "JAVA_HOME": "/usr/lib/jvm/java-21-openjdk",
        "PATH": "/usr/lib/jvm/java-21-openjdk/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
      },
      "health": {
        "endpoint": "/health"
      },
      "packages": [
        "openjdk-21-jre"
      ],
      "ports": [
        8080
      ],
      "workdir": "/app"
    },
    "version": "1.0"
  }
]
