[
  {
    "build": {
      "cache": [
        ".gradle",
        "build"
      ],
      "commands": [
        "gradle assemble -x test --no-daemon --console=plain"
      ],
      "env": {
        "GRADLE_OPTS": "-Dorg.gradle.native=false",
        "GRADLE_USER_HOME": "/root/.gradle",
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk"
      },
      "packages": [
        "openjdk-17",
        "gradle-9",
        "ca-certificates"
      ]
    },
    "metadata": {
      "build_system": "Gradle",
      "framework": "Spring Boot",
      "language": "Java",
      "project_name": "app",
      "reasoning": "Detected from build.gradle in "
    },
    "runtime": {
      "command": [
        "java",
        "-jar",
        "/app/app.jar"
      ],
      "copy": [
        {
          "from": "build/libs/*.jar",
          "to": "/app/"
        }
      ],
      "env": {
        "JAVA_HOME": "/usr/lib/jvm/java-17-openjdk",
        "PATH": "/usr/lib/jvm/java-17-openjdk/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
      },
      "health": {
        "endpoint": "/health"
      },
      "packages": [
        "openjdk-17-jre"
      ],
      "ports": [
        8080
      ],
      "workdir": "/app"
    },
    "version": "1.0"
  }
]
