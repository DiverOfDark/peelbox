[
  {
    "build": {
      "cache": [
        ".composer/cache",
        "vendor"
      ],
      "commands": [
        "composer config allow-plugins.symfony/runtime true",
        "composer install --no-dev --optimize-autoloader --ignore-platform-reqs"
      ],
      "env": {
        "COMPOSER_HOME": "/tmp/.composer",
        "HOME": "/tmp"
      },
      "packages": [
        "php-8.1",
        "composer",
        "unzip",
        "git",
        "ca-certificates",
        "php-8.1-ctype",
        "php-8.1-phar",
        "php-8.1-openssl",
        "php-8.1-mbstring",
        "php-8.1-xml",
        "php-8.1-dom",
        "php-8.1-curl"
      ]
    },
    "metadata": {
      "build_system": "Composer",
      "framework": "Symfony",
      "language": "PHP",
      "project_name": "app",
      "reasoning": "Detected from composer.json in "
    },
    "runtime": {
      "command": [
        "/usr/bin/php",
        "-S",
        "0.0.0.0:8000",
        "-t",
        "public"
      ],
      "copy": [
        {
          "from": ".",
          "to": "/app"
        }
      ],
      "env": {},
      "health": {
        "endpoint": "/_health"
      },
      "packages": [
        "php-8.1",
        "php-8.1-ctype",
        "php-8.1-phar",
        "php-8.1-openssl",
        "php-8.1-mbstring",
        "php-8.1-xml",
        "php-8.1-dom",
        "php-8.1-curl",
        "php-8.1-fileinfo",
        "php-8.1-iconv",
        "php-8.1-intl",
        "php-8.1-pdo"
      ],
      "ports": [
        8000
      ]
    },
    "version": "1.0"
  }
]
