[
  {
    "build": {
      "cache": [
        "target",
        ".cargo"
      ],
      "commands": [
        "cargo build --release --no-default-features -p peelbox-cli"
      ],
      "env": {
        "CARGO_HOME": ".cargo"
      },
      "packages": [
        "rust-1.92",
        "build-base",
        "openssl-dev",
        "pkgconf",
        "protoc"
      ]
    },
    "metadata": {
      "build_system": "Cargo",
      "language": "Rust",
      "project_name": "peelbox-cli",
      "reasoning": "Manual update for workspace structure"
    },
    "runtime": {
      "command": [
        "peelbox"
      ],
      "copy": [
        {
          "from": "target/release/peelbox",
          "to": "/usr/bin/peelbox"
        }
      ],
      "env": {
        "PATH": "/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
      },
      "packages": [
        "glibc",
        "ca-certificates",
        "openssl"
      ],
      "ports": []
    },
    "version": "1.0"
  }
]
