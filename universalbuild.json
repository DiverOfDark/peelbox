[
  {
    "build": {
      "cache": [
        "target",
        ".cargo"
      ],
      "commands": [
        "cargo build --release --no-default-features"
      ],
      "env": {
        "CARGO_HOME": ".cargo"
      },
      "packages": [
        "rust-1.92",
        "build-base",
        "openssl-dev"
      ]
    },
    "metadata": {
      "build_system": "Cargo",
      "language": "Rust",
      "project_name": "peelbox",
      "reasoning": "Detected from Cargo.toml in "
    },
    "runtime": {
      "command": [
        "/usr/local/bin/peelbox"
      ],
      "copy": [
        {
          "from": "target/release/peelbox",
          "to": "/usr/local/bin/peelbox"
        }
      ],
      "env": {},
      "packages": [
        "glibc",
        "ca-certificates"
      ],
      "ports": []
    },
    "version": "1.0"
  }
]
