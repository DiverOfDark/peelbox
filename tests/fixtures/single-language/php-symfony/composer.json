{
    "name": "example/symfony-api",
    "description": "Symfony API Application",
    "type": "project",
    "require": {
        "php": ">=8.1",
        "symfony/framework-bundle": "^6.4",
        "symfony/http-kernel": "^6.4",
        "symfony/runtime": "^6.4",
        "symfony/routing": "^6.4",
        "symfony/yaml": "^6.4"
    },
    "require-dev": {
        "symfony/dotenv": "^6.4",
        "phpunit/phpunit": "^10.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        }
    }
}
