{
  "name": "test-node-project",
  "version": "1.0.0",
  "description": "A test Node.js project for aipack testing",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && esbuild src/index.ts --bundle --platform=node --outfile=dist/index.js",
    "test": "jest --coverage",
    "start": "node dist/index.js",
    "dev": "nodemon --exec ts-node src/index.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": ["test", "nodejs", "typescript"],
  "author": "Test Author",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "dotenv": "^16.0.3"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.17",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "ts-node": "^10.9.1",
    "nodemon": "^2.0.22",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8",
    "esbuild": "^0.17.19"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
