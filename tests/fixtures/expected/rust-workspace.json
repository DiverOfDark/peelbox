      "cache": ["target/", "/usr/local/cargo/registry/"],
      "artifacts": ["target/release/*"]
    },
    "runtime": {
      "base": "debian:bookworm-slim",
      "packages": ["ca-certificates", "libssl3"],
      "env": {},
      "copy": [
        {
          "from": "target/release/app",
          "to": "/usr/local/bin/app"
        }
      ],
      "command": ["/usr/local/bin/app"],
      "ports": [8080],
      "healthcheck": null
    }
  }
